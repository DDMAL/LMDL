  GRAPH <urn:simssadb> {
    <https://db.simssa.ca/musicalworks/27> wdt:P136 ?genre.
    ?other_work wdt:P136 ?genre.
    FILTER (?other_work != <https://db.simssa.ca/musicalworks/27>)
  }
}

